{
    "suggest.minTriggerInputLength": 1,
    "coc.preferences.extensionUpdateCheck": "daily",
    "codeLens.enable": true,
    // diagnostics
    "diagnostic.errorSign": " ",
    "diagnostic.warningSign": " ",
    "diagnostic.infoSign": "",
    "diagnostic.hintSign": " ",
    "suggest.completionItemKindLabels": {
        "text": "t",
        "method": "m",
        "function": ""
    },
    "suggest.languageSourcePriority": 0,
    "coc.source.file.priority": 2,
    "coc.source.around.priority": 3,
    "coc.source.buffer.priority": 4,
    // autoformat
    "coc.preferences.formatOnSaveFiletypes": [
        "markdown",
        "yaml",
        "json"
    ],
    // list
    "list.indicator": ">",
    "list.selectedSignText": " ",
    "coc.preferences.hoverTarget": "float",
    "java.jdt.ls.vmargs": "-javaagent:/usr/share/java/lombok/lombok.jar -Xbootclasspath/a:/usr/share/java/lombok/lombok.jar",
    "java.import.maven.enabled": true,
    "java.import.gradle.enabled": true,
    "java.home": "/usr/lib/jvm/default",
    "java.completion.enabled": true,
    //"java.completion.maxResults": 10,
    "java.completion.guessMethodArguments": true,
    "java.progressReports.enabled": true,
    "java.implementationsCodeLens.enabled": true,
    "java.referencesCodeLens.enabled": true,
    "java.autobuild.enabled": true,
    "java.format.enabled": true,
    //"java.format.settings.url": "",
    "java.format.onType.enabled": true,
    "java.saveActions.organizeImports": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "suggest.noselect": false,
    "diagnostic.displayByAle": false,
    "diagnostic.checkCurrentLine": true,
    "sql.database": "postgresql"
}
